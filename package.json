{
  "name": "@brdemorin/google-docs-mcp-server",
  "version": "0.2.2",
  "description": "A powerful Model Context Protocol (MCP) server implementation for seamless Google Docs API integration, enabling AI assistants to create, read, update, and manage Google Docs with markdown export support",
  "main": "dist/index.js",
  "bin": {
    "google-docs-mcp-server": "dist/index.js"
  },
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "stdio": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "google-docs",
    "docs-api",
    "model-context-protocol",
    "ai-assistant",
    "google-api",
    "document-management",
    "document-editing",
    "typescript",
    "collaboration",
    "content-creation",
    "ai-tools",
    "markdown"
  ],
  "author": "Brian Morin <brdemorin@yahoo.com>",
  "contributors": [
    "lakhvinder singh (original author)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brdemorin/google-docs-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/brdemorin/google-docs-mcp-server/issues"
  },
  "homepage": "https://github.com/brdemorin/google-docs-mcp-server#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.8.0",
    "googleapis": "^133.0.0",
    "dotenv": "^16.4.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
